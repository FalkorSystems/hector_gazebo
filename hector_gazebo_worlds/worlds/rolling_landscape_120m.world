    <xyz>0 0 0</xyz>
    <rpy>0 20 20</rpy>
    <enableGravity>false</enableGravity>
    <light>
      <type>directional</type>
      <diffuseColor>1 1 1</diffuseColor>
      <specularColor>1 1 1</specularColor>
